{
  "name": "core-ws",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean _build:tokens _build:lib _build:storybook",
    "build:lib": "npm-run-all clean _build:tokens _build:lib",
    "build:watch": "npm run _build:lib -- --watch",
    "clean": "rimraf ./dist",
    "public-api:check": "filessert ./dist/lib/custom-elements.json ./custom-elements.json",
    "public-api:update": "npm run build:lib && cpy ./dist/lib/custom-elements.json ./",
    "sandbox": "web-dev-server --config ./web-dev-server.sandbox.mjs",
    "start": "npm-run-all build --parallel build:watch _storybook:start",
    "test": "npm-run-all _test:unit _test:performance",
    "test:coverage": "npm-run-all _clean:coverage _test:unit_coverage",
    "test:watch": "wtr --watch",
    "_build:lib": "rollup --config ./rollup.config.js",
    "_build:storybook": "build-storybook --config-dir ./.storybook --output-dir ./dist/storybook",
    "_build:tokens": "ts-node ./build/build.tokens.ts",
    "_clean:coverage": "rimraf ./dist/coverage",
    "_storybook:start": "web-dev-server --config ./web-dev-server.storybook.mjs",
    "_test:a11y": "wtr --config ./web-test-runner.a11y.mjs",
    "_test:performance": "wtr --config ./web-test-runner.performance.mjs",
    "_test:unit": "wtr",
    "_test:unit:coverage": "npm run _test:unit -- --coverage"
  },
  "customElements": "./dist/lib/custom-elements.json",
  "sideEffects": [
    "./src/**/*.register.js"
  ],
  "files": [
    "*"
  ],
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@cds/city": "^1.1.0",
    "@custom-elements-manifest/analyzer": "0.5.7",
    "@open-wc/dev-server-hmr": "0.1.2",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-image": "2.1.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-replace": "3.0.1",
    "@rollup/plugin-typescript": "8.3.0",
    "@rollup/plugin-virtual": "2.0.3",
    "@skypack/package-check": "0.2.2",
    "@types/jasmine": "3.10.3",
    "@types/jasmine-expect": "3.8.1",
    "@types/jasmine-matchers": "0.2.32",
    "@types/node": "14.17.34",
    "@types/ramda": "0.27.11",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "@web/dev-server": "0.1.29",
    "@web/dev-server-esbuild": "0.2.16",
    "@web/dev-server-import-maps": "0.0.6",
    "@web/dev-server-rollup": "0.3.15",
    "@web/dev-server-storybook": "0.4.2",
    "@web/test-runner": "0.13.26",
    "@web/test-runner-commands": "0.6.1",
    "@web/test-runner-playwright": "0.8.8",
    "autoprefixer": "10.4.2",
    "browserslist": "4.19.1",
    "bundlesize": "1.0.0-beta.2",
    "cem-plugin-module-file-extensions": "0.0.2",
    "cem-plugin-readonly": "0.0.2",
    "csso": "5.0.2",
    "eslint-plugin-lit": "1.6.1",
    "eslint-plugin-lit-a11y": "2.2.0",
    "eslint-plugin-rulesdir": "0.2.1",
    "eslint-plugin-wc": "1.3.2",
    "filessert": "^0.0.1",
    "fs-extra": "10.0.0",
